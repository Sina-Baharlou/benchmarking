{
  "metadata": {
    "title": "Learn WN18 Data Set with ComplEx as described by Trouillon et al., 2016",
    "comments": "In the paper it is not mentioned that they compute the mean of the vector norms as done in the published code base."
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "ComplEx",
    "model_kwargs": {
      "embedding_dim": 150
    },
    "regularizer": "Lp",
    "regularizer_kwargs": {
      "weight": 0.03,
      "p": 2.0,
      "normalize": false
    },
    "optimizer": "AdaGrad",
    "optimizer_kwargs": {
      "lr": 0.5
    },
    "loss": "SoftplusLoss",
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 4831
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}
