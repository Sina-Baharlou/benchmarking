{
  "metadata": {
    "title": "HPO Over YAGO3-10 for TransH"
  },
  "ablation": {
    "datasets": [
      "yago310"
    ],
    "models": [
      "TransH"
    ],
    "model_kwargs": {
      "TransH": {
        "automatic_memory_optimization": true
      }
    },
    "model_kwargs_ranges": {
      "TransH": {
        "embedding_dim": {
          "type": "int",
          "low": 64,
          "high": 192,
          "q": 64
        },
        "scoring_fct_norm": {
          "type": "int",
          "low": 1,
          "high": 2
        }
      }
    },
    "training_loops": [
      "owa"
    ],
    "optimizers": [
      "adam",
      "adadelta"
    ],
    "optimizer_kwargs": {
      "TransH": {
        "adam": {
          "weight_decay": 0.0
        },
        "adadelta": {
          "weight_decay": 0.0
        }
      }
    },
    "optimizer_kwargs_ranges": {
      "TransH": {
        "adam": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        },
        "adadelta": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        }
      }
    },
    "loss_functions": [
      "MarginRankingLoss"
    ],
    "loss_kwargs": {
      "TransH": {
        "MarginRankingLoss": {}
      }
    },
    "loss_kwargs_ranges": {
      "TransH": {
        "MarginRankingLoss": {
          "margin": {
            "type": "float",
            "low": 0.5,
            "high": 10,
            "q": 1.0
          }
        }
      }
    },
    "regularizers": [
      "TransH"
    ],
    "regularizer_kwargs": {
      "TransH": {
        "TransH": {
          "epsilon": 1e-05
        }
      }
    },
    "regularizer_kwargs_ranges": {
      "TransH": {
        "TransH": {
          "weight": {
            "type": "float",
            "low": 0.01,
            "high": 0.3,
            "scale": "log"
          }
        }
      }
    },
    "negative_sampler": "BasicNegativeSampler",
    "negative_sampler_kwargs": {
      "TransH": {
        "BasicNegativeSampler": {}
      }
    },
    "negative_sampler_kwargs_ranges": {
      "TransH": {
        "BasicNegativeSampler": {
          "num_negs_per_pos": {
            "type": "int",
            "low": 1,
            "high": 100,
            "q": 5
          }
        }
      }
    },
    "create_inverse_triples": [
      true,
      false
    ],
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 1
    },
    "training_kwargs": {
      "TransH": {
        "owa": {
          "num_epochs": 1000,
          "label_smoothing": 0.0
        }
      }
    },
    "training_kwargs_ranges": {
      "TransH": {
        "owa": {
          "batch_size": {
            "type": "int",
            "low": 7,
            "high": 9,
            "scale": "power_two"
          }
        }
      }
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}