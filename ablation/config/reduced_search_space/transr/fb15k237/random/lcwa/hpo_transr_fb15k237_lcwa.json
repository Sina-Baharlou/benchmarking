{
  "metadata": {
    "title": "HPO Over FB15K-237 for TransR"
  },
  "ablation": {
    "datasets": [
      "fb15k237"
    ],
    "models": [
      "TransR"
    ],
    "model_kwargs": {
      "TransR": {
        "automatic_memory_optimization": true
      }
    },
    "model_kwargs_ranges": {
      "TransR": {
        "embedding_dim": {
          "type": "int",
          "low": 64,
          "high": 192,
          "q": 64
        },
        "relation_dim": {
          "type": "int",
          "low": 64,
          "high": 192,
          "q": 64
        },
        "scoring_fct_norm": {
          "type": "int",
          "low": 1,
          "high": 2
        }
      }
    },
    "training_loops": [
      "lcwa"
    ],
    "optimizers": [
      "adam",
      "adadelta"
    ],
    "optimizer_kwargs": {
      "TransR": {
        "adam": {
          "weight_decay": 0.0
        },
        "adadelta": {
          "weight_decay": 0.0
        }
      }
    },
    "optimizer_kwargs_ranges": {
      "TransR": {
        "adam": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        },
        "adadelta": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        }
      }
    },
    "loss_functions": [
      "BCEAfterSigmoidLoss",
      "SoftplusLoss"
    ],
    "loss_kwargs": {
      "TransR": {
        "BCEAfterSigmoidLoss": {},
        "SoftplusLoss": {}
      }
    },
    "loss_kwargs_ranges": {
      "TransR": {
        "BCEAfterSigmoidLoss": {},
        "SoftplusLoss": {}
      }
    },
    "regularizers": [
      "NoRegularizer"
    ],
    "regularizer_kwargs": {
      "TransR": {
        "NoRegularizer": {}
      }
    },
    "regularizer_kwargs_ranges": {
      "TransR": {
        "NoRegularizer": {}
      }
    },
    "create_inverse_triples": [
      true,
      false
    ],
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 1
    },
    "training_kwargs": {
      "TransR": {
        "lcwa": {
          "num_epochs": 1000
        }
      }
    },
    "training_kwargs_ranges": {
      "TransR": {
        "lcwa": {
          "label_smoothing": {
            "type": "float",
            "low": 0.001,
            "high": 1.0,
            "scale": "log"
          },
          "batch_size": {
            "type": "int",
            "low": 7,
            "high": 9,
            "scale": "power_two"
          }
        }
      }
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}