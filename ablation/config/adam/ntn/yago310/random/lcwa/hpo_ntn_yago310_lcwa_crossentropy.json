{
  "metadata": {
    "title": "HPO Over YAGO3-10 for NTN"
  },
  "ablation": {
    "datasets": [
      "yago310"
    ],
    "models": [
      "NTN"
    ],
    "model_kwargs": {
      "NTN": {
        "num_slices": 4,
        "automatic_memory_optimization": true
      }
    },
    "model_kwargs_ranges": {
      "NTN": {
        "embedding_dim": {
          "type": "int",
          "low": 6,
          "high": 8,
          "scale": "power_two"
        }
      }
    },
    "training_loops": [
      "lcwa"
    ],
    "optimizers": [
      "adam"
    ],
    "optimizer_kwargs": {
      "NTN": {
        "adam": {
          "weight_decay": 0.0
        }
      }
    },
    "optimizer_kwargs_ranges": {
      "NTN": {
        "adam": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        }
      }
    },
    "loss_functions": [
      "CrossEntropyLoss"
    ],
    "loss_kwargs": {
      "NTN": {
        "CrossEntropyLoss": {}
      }
    },
    "loss_kwargs_ranges": {
      "NTN": {
        "CrossEntropyLoss": {}
      }
    },
    "regularizers": [
      "NoRegularizer"
    ],
    "regularizer_kwargs": {
      "NTN": {
        "NoRegularizer": {}
      }
    },
    "regularizer_kwargs_ranges": {
      "NTN": {
        "NoRegularizer": {}
      }
    },
    "create_inverse_triples": [
      true,
      false
    ],
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": null
    },
    "training_kwargs": {
      "NTN": {
        "lcwa": {
          "num_epochs": 1000
        }
      }
    },
    "training_kwargs_ranges": {
      "NTN": {
        "lcwa": {
          "label_smoothing": {
            "type": "float",
            "low": 0.001,
            "high": 1.0,
            "scale": "log"
          },
          "batch_size": {
            "type": "int",
            "low": 7,
            "high": 9,
            "scale": "power_two"
          }
        }
      }
    },
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}