{
  "metadata": {
    "title": "HPO Over WN18RR for DistMult"
  },
  "pipeline": {
    "dataset": "wn18rr",
    "dataset_kwargs": {
      "create_inverse_triples": true
    },
    "model": "DistMult",
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 50,
        "high": 500,
        "q": 100
      }
    },
    "loss": "BCEAfterSigmoidLoss",
    "regularizer": "Lp",
    "regularizer_kwargs": {
      "weight": 0.0001,
      "p": 2.0,
      "normalize": false
    },
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "owa",
    "negative_sampler": "BasicNegativeSampler",
    "negative_sampler_kwargs_ranges": {
      "num_negs_per_pos": {
        "type": "int",
        "low": 1,
        "high": 100,
        "q": 10
      }
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    },
    "training_kwargs": {
      "batch_size": 256,
      "label_smoothing": 0.0
    },
    "training_kwargs_ranges": {
      "num_epochs": {
        "type": "int",
        "low": 500,
        "high": 1000,
        "q": 100
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}