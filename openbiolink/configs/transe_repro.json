{
  "metadata": {
    "title": "Learn OpenBioLink Data Set with TransE as described by Breitner et al., 2020",
    "comments": ""
  },
  "pipeline": {
    "dataset": "openbiolink",
    "model": "TransE",
    "model_kwargs": {
      "embedding_dim": 100,
      "scoring_fct_norm": 1
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "reduction": "mean",
      "margin": 2
    },
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 2048
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  }
}