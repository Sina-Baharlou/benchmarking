{
  "metadata": {
    "title": "HPO Over WN18RR for TransE"
  },
  "pipeline": {
    "dataset": "wn18rr",
    "dataset_kwargs": {
      "create_inverse_triples": true
    },
    "model": "TransE",
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 50,
        "high": 200,
        "q": 50
      }
    },
    "loss": "BCEAfterSigmoidLoss",
    "regularizer": "NoRegularizer",
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "lcwa",
    "training_kwargs": {
      "batch_size": 256
    },
    "training_kwargs_ranges": {
      "num_epochs": {
        "type": "int",
        "low": 500,
        "high": 2000,
        "q": 100
      },
      "label_smoothing": {
        "type": "float",
        "low": 0.001,
        "high": 1.0,
        "scale": "log"
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 1
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}