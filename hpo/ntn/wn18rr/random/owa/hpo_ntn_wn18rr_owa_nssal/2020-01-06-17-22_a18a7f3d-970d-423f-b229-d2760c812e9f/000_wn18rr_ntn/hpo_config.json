{
  "metadata": {
    "title": "HPO Over WN18RR for NTN"
  },
  "pipeline": {
    "dataset": "wn18rr",
    "dataset_kwargs": {
      "create_inverse_triples": false
    },
    "model": "NTN",
    "model_kwargs": {
      "num_slices": 4
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 50,
        "high": 500,
        "q": 100
      }
    },
    "loss": "NegativeSamplingSelfAdversarialLoss",
    "loss_kwargs_ranges": {
      "margin": {
        "type": "float",
        "low": 1,
        "high": 30,
        "q": 2.0
      },
      "adversarial_temperature": {
        "type": "float",
        "low": 0.1,
        "high": 1.0,
        "q": 0.1
      }
    },
    "regularizer": "NoRegularizer",
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "owa",
    "negative_sampler": "BasicNegativeSampler",
    "negative_sampler_kwargs_ranges": {
      "num_negs_per_pos": {
        "type": "int",
        "low": 10,
        "high": 100,
        "q": 10
      }
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    },
    "training_kwargs": {
      "batch_size": 256,
      "label_smoothing": 0.0
    },
    "training_kwargs_ranges": {
      "num_epochs": {
        "type": "int",
        "low": 500,
        "high": 2000,
        "q": 100
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 1
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}